from typing import Optional, List
from fastapi import FastAPI, Response, status, HTTPException, APIRouter, Depends
from sqlalchemy.orm import Session
from ...models import schemas, sa_models
from ...utilities import oauth2, utils
from ... import database

router = APIRouter(
    prefix="/prompts",
    tags=["Promts"]
)

@router.get("/", response_model=[schemas.PromtElementOut])
def get_prompt_elements(db: Session = Depends(database.get_db), current_user: int = Depends(oauth2.get_current_user)):
    #unfinished come back after journal entry 
    prompt_elements = db.query(sa_models.PromptElement).all()

    return prompt_elements
